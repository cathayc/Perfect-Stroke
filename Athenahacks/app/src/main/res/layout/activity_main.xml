<?xml version="1.0" encoding="utf-8"?>
<android.support.v7.widget.FitWindowsLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.cathychang.athenahacks.MainActivity">

    <ImageView
        android:id="@+id/my_image_view"
        android:src="@drawable/strokebeauty"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"/>

    <Button
        android:id="@+id/start"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Are you ready to begin?"
        tools:layout_editor_absoluteX="24dp"

        tools:layout_editor_absoluteY="78dp" />

</android.support.v7.widget.FitWindowsLinearLayout>
